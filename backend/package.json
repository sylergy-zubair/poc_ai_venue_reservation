{
  "name": "venue-booking-backend",
  "version": "1.0.0",
  "description": "AI-powered venue booking backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest --watchAll=false",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:integration": "jest --testPathPattern=integration --watchAll=false",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "migrate": "node dist/scripts/migrate.js",
    "migrate:rollback": "node dist/scripts/rollback.js",
    "seed": "node dist/scripts/seed.js"
  },
  "keywords": ["venue", "booking", "ai", "claude", "api"],
  "author": "Venue Booking Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1",
    "axios": "^1.6.2",
    "redis": "^4.6.11",
    "compression": "^1.7.4",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^9.0.7",
    "@types/compression": "^1.7.5",
    "@types/multer": "^1.4.11",
    "@types/node-cron": "^3.0.11",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^7.1.3",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "mongodb-memory-server": "^9.1.3",
    "cross-env": "^7.0.3",
    "tsconfig-paths": "^4.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}